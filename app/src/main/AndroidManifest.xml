<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="attendancetracker.ase.com.ase_attendancetracker">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activity.AttendanceListActivity"
            android:parentActivityName=".view.activity.LoginActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".view.activity.LoginActivity" />
        </activity>
        <activity
            android:name=".view.activity.DisplayQRCodeActivity"
            android:parentActivityName=".view.activity.DisplayQRCodeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".view.activity.DisplayQRCodeActivity" />
        </activity>
        <activity
            android:name=".view.activity.HomeActivity"
            android:label="Advance Software Engineering"></activity>
        <activity android:name=".view.activity.AttendanceDetailsListActivity"></activity>
    </application>

</manifest>